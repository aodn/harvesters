  <node componentName="iPostgresqlDbUpdate" componentVersion="0.1" offsetLabelX="0" offsetLabelY="0" posX="224" posY="192">
    <elementParameter field="TEXT" name="UNIQUE_NAME" value="iPostgresqlDbUpdate_1"/>
    <elementParameter field="TEXT" name="HOST" value="context.harvestDb_Server"/>
    <elementParameter field="TEXT" name="TYPE" value="POSTGRESQL"/>
    <elementParameter field="TEXT" name="PORT" value="context.harvestDb_Port"/>
    <elementParameter field="TEXT" name="DBNAME" value="context.harvestDb_Database"/>
    <elementParameter field="TEXT" name="SCHEMA_DB" value="context.harvestDb_Schema"/>
    <elementParameter field="TEXT" name="USER" value="context.harvestDb_Login"/>
    <elementParameter field="TEXT" name="PASS" value="context.harvestDb_Password"/>
    <elementParameter field="FILE" name="FILENAME" value="&quot;/home/mhidas/work/sql/data_harvest/anmn_ts.changelog.xml&quot;"/>
    <elementParameter field="CHECK" name="USE_INLINE_CONTENT" value="true"/>
    <elementParameter field="MEMO" name="INLINE_CONTENT" value="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot; standalone=&quot;no&quot;?&gt;&#10;&lt;databaseChangeLog xmlns=&quot;http://www.liquibase.org/xml/ns/dbchangelog&quot; xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot; xsi:schemaLocation=&quot;http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd&quot;&gt;&#10;&#10;    &lt;changeSet author=&quot;talend&quot; id=&quot;create_timeseries_table-1&quot;&gt;&#10;      &lt;sql&gt;&#10;&#9;CREATE TABLE anmn_ts.timeseries&#10;&#9;(&#10;&#9;  id bigserial NOT NULL,&#10;&#9;  file_id bigint NOT NULL,&#10;&#9;  site_code text NOT NULL,&#10;&#9;  platform_code text NOT NULL,&#10;&#9;  deployment_code text NOT NULL,&#10;&#9;  &quot;LATITUDE&quot; double precision,&#10;&#9;  &quot;LATITUDE_quality_control&quot; character(1),&#10;&#9;  &quot;LONGITUDE&quot; double precision,&#10;&#9;  &quot;LONGITUDE_quality_control&quot; character(1),&#10;&#9;  geom geometry(Geometry,4326),&#10;&#9;  instrument_nominal_depth real,&#10;&#9;  site_nominal_depth real,&#10;&#9;  site_depth_at_deployment real,&#10;&#9;  instrument text,&#10;&#9;  instrument_serial_number text,&#10;&#9;  time_coverage_start timestamp with time zone,&#10;&#9;  time_coverage_end timestamp with time zone,&#10;&#9;  time_deployment_start timestamp with time zone,&#10;&#9;  time_deployment_end timestamp with time zone,&#10;&#9;  comment text,&#10;&#9;  history text,&#10;&#9;  toolbox_version text,&#10;&#9;  depth_b boolean,&#10;&#9;  sea_water_temperature_b boolean,&#10;&#9;  sea_water_pressure_b boolean,&#10;&#9;  sea_water_pressure_due_to_sea_water_b boolean,&#10;&#9;  CONSTRAINT timeseries_pk PRIMARY KEY (id ),&#10;&#9;  CONSTRAINT timeseries_file_fk FOREIGN KEY (file_id)&#10;&#9;      REFERENCES anmn_ts.indexed_file (id) MATCH SIMPLE&#10;&#9;      ON UPDATE CASCADE ON DELETE CASCADE,&#10;&#9;  CONSTRAINT timeseries_deployment_instrument_depth_uc UNIQUE (deployment_code, instrument, instrument_nominal_depth),&#10;&#9;  CONSTRAINT timeseries_deployment_serial_number_uc UNIQUE (deployment_code, instrument_serial_number)&#10;&#9;)&#10;&#9;WITH (&#10;&#9;  OIDS=FALSE&#10;&#9;);&#10;&#9;ALTER TABLE anmn_ts.timeseries&#10;&#9;  OWNER TO anmn_ts;&#10;      &lt;/sql&gt;&#10;    &lt;/changeSet&gt;&#10;&#10;&#10;    &lt;changeSet author=&quot;talend&quot; id=&quot;create_measurement_table-1&quot;&gt;&#10;      &lt;sql&gt;&#10;&#9;CREATE TABLE anmn_ts.measurement&#10;&#9;(&#10;&#9;  ts_id bigint NOT NULL,&#10;&#9;  index bigint NOT NULL,&#10;&#9;  &quot;TIME&quot; timestamp with time zone NOT NULL,&#10;&#9;  &quot;TIME_quality_control&quot; character(1),&#10;&#9;  &quot;DEPTH&quot; real,&#10;&#9;  &quot;DEPTH_quality_control&quot; character(1),&#10;&#9;  &quot;TEMP&quot; real,&#10;&#9;  &quot;TEMP_quality_control&quot; character(1),&#10;&#9;  &quot;PRES&quot; real,&#10;&#9;  &quot;PRES_quality_control&quot; character(1),&#10;&#9;  &quot;PRES_REL&quot; real,&#10;&#9;  &quot;PRES_REL_quality_control&quot; character(1),&#10;&#9;  CONSTRAINT measurement_fk PRIMARY KEY (ts_id , index ),&#10;&#9;  CONSTRAINT measurement_ts_fk FOREIGN KEY (ts_id)&#10;&#9;      REFERENCES anmn_ts.timeseries (id) MATCH SIMPLE&#10;&#9;      ON UPDATE CASCADE ON DELETE CASCADE&#10;&#9;)&#10;&#9;WITH (&#10;&#9;  OIDS=FALSE&#10;&#9;);&#10;&#9;ALTER TABLE anmn_ts.measurement&#10;&#9;  OWNER TO anmn_ts;&#10;&#9;COMMENT ON COLUMN anmn_ts.measurement.index IS 'index of measurement in netCDF file';&#10;      &lt;/sql&gt;&#10;    &lt;/changeSet&gt;&#10;&#10;&#10;    &lt;changeSet author=&quot;talend&quot; id=&quot;update_geometry_column-1&quot;&gt;&#10;      &lt;sql&gt;&#10;&#9;ALTER TABLE timeseries&#10;&#9;  ALTER COLUMN geom TYPE geometry(GEOMETRY, 4326)&#10;&#9;  USING ST_SetSRID(geom,4326);&#10;&#9;CREATE INDEX timeseries_gist_idx ON timeseries USING GIST (geom); &#10;      &lt;/sql&gt;&#10;    &lt;/changeSet&gt;&#10;&#10;&#10;    &lt;changeSet author=&quot;talend&quot; id=&quot;create_map_view-1&quot;&gt;&#10;      &lt;sql&gt;&#10;&#9;CREATE OR REPLACE VIEW anmn_ts.anmn_ts_timeseries_map AS &#10;&#9;  SELECT &#10;&#9;    t.id AS timeseries_id,&#10;&#9;    f.url AS file_url,&#10;&#9;    -- date(f.first_indexed AT TIME ZONE 'UTC') AS date_published,&#10;&#9;    t.site_code,&#10;&#9;    t.platform_code,&#10;&#9;    t.deployment_code,&#10;&#9;    t.geom,&#10;&#9;    t.&quot;LATITUDE&quot;,&#10;&#9;    t.&quot;LONGITUDE&quot;,&#10;&#9;    t.instrument_nominal_depth,&#10;&#9;    t.time_deployment_start AT TIME ZONE 'UTC' AS time_deployment_start,&#10;&#9;    t.time_deployment_end AT TIME ZONE 'UTC' AS time_deployment_end,&#10;&#9;    t.instrument,&#10;&#9;    t.instrument_serial_number,&#10;&#9;    t.depth_b,&#10;&#9;    t.sea_water_temperature_b,&#10;&#9;    t.sea_water_pressure_b,&#10;&#9;    t.sea_water_pressure_due_to_sea_water_b&#10;&#9;    FROM anmn_ts.timeseries t JOIN anmn_ts.indexed_file f ON t.file_id = f.id;&#10;&#10;&#9;ALTER TABLE anmn_ts.anmn_ts_timeseries_map&#10;&#9;  OWNER TO anmn_ts;&#10;&#9;GRANT ALL ON TABLE anmn_ts.anmn_ts_timeseries_map TO anmn_ts;&#10;      &lt;/sql&gt;&#10;    &lt;/changeSet&gt;&#10;&#10;&#10;    &lt;changeSet author=&quot;talend&quot; id=&quot;create_data_view-1&quot;&gt;&#10;      &lt;sql&gt;&#10;&#9;CREATE OR REPLACE VIEW anmn_ts.anmn_ts_timeseries_data AS &#10;&#9;  SELECT &#10;&#9;    m.ts_id AS timeseries_id,&#10;&#9;    m.index,&#10;&#9;    t.site_code,&#10;&#9;    t.platform_code,&#10;&#9;    t.deployment_code,&#10;&#9;    t.instrument_nominal_depth,&#10;&#9;    m.&quot;TIME&quot; AT TIME ZONE 'UTC' AS &quot;TIME&quot;,&#10;&#9;    m.&quot;TIME_quality_control&quot;,&#10;&#9;    t.&quot;LATITUDE&quot;,&#10;&#9;    t.&quot;LATITUDE_quality_control&quot;,&#10;&#9;    t.&quot;LONGITUDE&quot;,&#10;&#9;    t.&quot;LONGITUDE_quality_control&quot;,&#10;&#9;    m.&quot;DEPTH&quot;,&#10;&#9;    m.&quot;DEPTH_quality_control&quot;,&#10;&#9;    m.&quot;TEMP&quot;,&#10;&#9;    m.&quot;TEMP_quality_control&quot;,&#10;&#9;    m.&quot;PRES&quot;,&#10;&#9;    m.&quot;PRES_quality_control&quot;,&#10;&#9;    m.&quot;PRES_REL&quot;,&#10;&#9;    m.&quot;PRES_REL_quality_control&quot;,&#10;&#9;    t.geom,&#10;&#9;    t.depth_b,&#10;&#9;    t.sea_water_temperature_b,&#10;&#9;    t.sea_water_pressure_b,&#10;&#9;    t.sea_water_pressure_due_to_sea_water_b&#10;&#9;    FROM  anmn_ts.timeseries t  JOIN  anmn_ts.measurement m  ON  t.id = m.ts_id;&#10;&#10;&#9;ALTER TABLE anmn_ts.anmn_ts_timeseries_data&#10;&#9;  OWNER TO anmn_ts;&#10;&#9;GRANT ALL ON TABLE anmn_ts.anmn_ts_timeseries_data TO anmn_ts;&#10;      &lt;/sql&gt;&#10;    &lt;/changeSet&gt;&#10;&#10;&#10;&lt;/databaseChangeLog&gt;&#10;"/>
