&#x9;size,&#xA;&#x9;&#x9;&#x9;data_centre_name,&#xA;&#x9;&#x9;&#x9;oxygen_sensor,&#xA;&#x9;&#x9;&#x9;platform_number,&#xA;&#x9;&#x9;&#x9;cycle_number,&#xA;&#x9;&#x9;&#x9;direction,&#xA;&#x9;&#x9;&#x9;data_state_indicator,&#xA;&#x9;&#x9;&#x9;data_mode,&#xA;&#x9;&#x9;&#x9;juld,&#xA;&#x9;&#x9;&#x9;juld_qc,&#xA;&#x9;&#x9;&#x9;juld_location,&#xA;&#x9;&#x9;&#x9;latitude,&#xA;&#x9;&#x9;&#x9;longitude,&#xA;&#x9;&#x9;&#x9;position_qc,&#xA;&#x9;&#x9;&#x9;position,&#xA;&#x9;&#x9;&#x9;project_name,&#xA;&#x9;&#x9;&#x9;profile_processing_type,&#xA;&#x9;&#x9;&#x9;profile_type,&#xA;&#x9;&#x9;&#x9;profile_no,&#xA;&#x9;&#x9;&#x9;level_no,&#xA;&#x9;&#x9;&#x9;pres,&#xA;&#x9;&#x9;&#x9;pres_qc,&#xA;&#x9;&#x9;&#x9;pres_adjusted,&#xA;&#x9;&#x9;&#x9;pres_adjusted_qc,&#xA;&#x9;&#x9;&#x9;pres_adjusted_error,&#xA;&#x9;&#x9;&#x9;doxy_adjusted,&#xA;&#x9;&#x9;&#x9;doxy_adjusted_qc,&#xA;&#x9;&#x9;&#x9;doxy_adjusted_error&#xA;&#x9;&#x9;&#x9;FROM argo_profile_data&#xA;&#x9;&#x9;&#x9;WHERE doxy_adjusted is NOT NULL and profile_type = 'Bio-argo profile');&#xA;&#x9;&#x9;&lt;/sql>   &#xA;&#xA;&#x9;&#x9;&lt;sql>&#xA;&#x9;&#x9;&#x9;CREATE MATERIALIZED VIEW argo_trajectory_map AS &#xA;&#x9;&#x9;&#x9;SELECT platform_number,&#xA;&#x9;&#x9;&#x9;date_trunc('quarter', juld) as datechunk,&#xA;&#x9;&#x9;&#x9;data_centre_name, &#xA;&#x9;&#x9;&#x9;profile_processing_type,&#xA;&#x9;&#x9;&#x9;oxygen_sensor,&#xA;&#x9;&#x9;&#x9;timezone('UTC'::text, min(juld)) AS time_coverage_start,&#xA;&#x9;&#x9;&#x9;timezone('UTC'::text, max(juld)) AS time_coverage_end,&#xA;&#x9;&#x9;&#x9;make_trajectory(&quot;position&quot; order by platform_number, juld) as traj,&#xA;&#x9;&#x9;&#x9;ST_Multi(ST_Union(profile_summary.&quot;position&quot;)) AS  mpoint &#xA;&#x9;&#x9;&#x9;FROM profile_summary WHERE profile_no = 1&#xA;&#x9;&#x9;&#x9;group by platform_number, data_centre_name,oxygen_sensor, profile_processing_type, datechunk;&#xA;&#x9;&#x9;&lt;/sql>&#xA;&#xA;&#x9;&#x9;&lt;sql>&#xA;&#x9;&#x9;&#x9;CREATE UNIQUE INDEX trajec_vw_idx ON argo_trajectory_map (platform_number, profile_processing_type, oxygen_sensor, datechunk);&#xA;&#x9;&#x9;&lt;/sql>&#xA;&#xA;&#x9;&#x9;&lt;sql>&#xA;&#x9;&#x9;&#x9;CREATE MATERIALIZED VIEW argo_float_aus_region_map AS &#xA;&#x9;&#x9;&#x9;SELECT platform_number, &#xA;&#x9;&#x9;&#x9;juld,&#xA;&#x9;&#x9;&#x9;data_centre_name,&#xA;&#x9;&#x9;&#x9;profile_processing_type,&#xA;&#x9;&#x9;&#x9;oxygen_sensor,      &#xA;&#x9;&#x9;&#x9;position &#xA;&#x9;&#x9;&#x9;FROM profile_summary &#xA;&#x9;&#x9;&#x9;WHERE profile_no = 1 AND position &amp;amp;&amp;amp; ST_MakeEnvelope(46,-70,135, 21, 4326) OR data_centre_name = 'CSIRO'&#xA;&#x9;&#x9;&#x9;group by platform_number, data_centre_name,oxygen_sensor, profile_processing_type, juld,position;&#xA;&#x9;&#x9;&lt;/sql>&#xA;&#xA;&#x9;&#x9;&lt;sql>&#xA;&#x9;&#x9;&#x9;CREATE UNIQUE INDEX pos_vw_idx ON argo_float_aus_region_map (platform_number, profile_processing_type, oxygen_sensor, juld,position);&#xA;&#x9;&#x9;&lt;/sql>&#xA;&#xA;&#x9;&#x9;&lt;sql>&#xA;&#x9;&#x9;&#x9;CREATE MATERIALIZED VIEW argo_float_aus_so_region_map AS&#xA;&#x9;&#x9;&#x9;SELECT profile_summary.platform_number,&#xA;&#x9;&#x9;&#x9;profile_summary.juld,&#xA;&#x9;&#x9;&#x9;profile_summary.data_centre_name,&#xA;&#x9;&#x9;&#x9;profile_summary.profile_processing_type,&#xA;&#x9;&#x9;&#x9;profile_summary.oxygen_sensor,&#xA;&#x9;&#x9;&#x9;profile_summary.position,&#xA;&#x9;&#x9;&#x9;indexed_file.url&#xA;&#x9;&#x9;&#x9;FROM argo.profile_summary&#xA;&#x9;&#x9;&#x9;LEFT JOIN argo.indexed_file ON profile_summary.file_id = indexed_file.id&#xA;&#x9;&#x9;&#x9;WHERE profile_summary.profile_no = 1 AND profile_type = 'Core-argo profile'&#xA;&#x9;&#x9;&#x9;AND (ST_Contains(st_makepolygon(st_geomfromtext('LINESTRING( -180 0, -170 0, -170 -40, 20 -40, 20 0, 180 0, 180 -85, -180 -85, -180 0)'::text, 4326)), position) OR data_centre_name = 'CSIRO'::text) &#xA;&#x9;&#x9;&#x9;GROUP BY indexed_file.url, profile_summary.platform_number, profile_summary.data_centre_name, profile_summary.oxygen_sensor, profile_summary.profile_processing_type, profile_summary.juld, profile_summary.position;&#xA;&#x9;&#x9;&lt;/sql>&#xA;&#xA;&#x9;&#x9;&lt;sql>&#xA;&#x9;&#x9;&#x9;CREATE UNIQUE INDEX pos_aus_so_vw_idx ON argo_float_aus_so_region_map (url,platform_number, profile_processing_type, oxygen_sensor, juld,position);&#xA;&#x9;&#x9;&lt;/sql>&#xA;&#xA;&#x9;&#x9;&lt;sql>&#xA;&#x9;&#x9;&#x9;CREATE INDEX float_aus_region_pos_idx  ON argo_float_aus_region_map USING gist (position);&#xA;&#x9;&#x9;&#x9;CREATE INDEX float_aus_region_time_idx  ON argo_float_aus_region_map USING btree (juld);&#xA;&#x9;&#x9;&lt;/sql>&#xA;&#xA;&#x9;&#x9;&lt;sql>&#xA;&#x9;&#x9;&#x9;CREATE INDEX float_aus_traj_idx ON argo_trajectory_map USING gist (mpoint);&#xA;&#x9;&#x9;&#x9;CREATE INDEX float_aus_traj_timeStart_idx  ON argo_trajectory_map USING btree (time_coverage_start);&#xA;&#x9;&#x9;&#x9;CREATE INDEX float_aus_traj_timeEnd_idx  ON argo_trajectory_map USING btree (time_coverage_end);&#xA;&#x9;&#x9;&lt;/sql>&#xA;&#xA;&#x9;&#x9;&lt;sql>&#xA;&#x9;&#x9;&#x9;CREATE INDEX float_aus_so_region_pos_idx  ON argo_float_aus_so_region_map USING gist (position);&#xA;&#x9;&#x9;&#x9;CREATE INDEX float_aus_so_region_time_idx  ON argo_float_aus_so_region_map USING btree (juld);&#xA;&#x9;&#x9;&lt;/sql>&#xA;&#x9;&lt;/changeSet>&#xA;&lt;/databaseChangeLog> &#xA;"/>
