    <elementParameter field="TEXT" name="DBNAME" value="context.Destination_Database"/>
    <elementParameter field="TEXT" name="SCHEMA_DB" value="context.Destination_Schema"/>
    <elementParameter field="TEXT" name="USER" value="context.Destination_Login"/>
    <elementParameter field="TEXT" name="PASS" value="context.Destination_Password"/>
    <elementParameter field="FILE" name="FILENAME" value="&quot;/opt/TOS_DI-r96646-V5.1.3/workspace_sdi/harvesters/workspace/changelog.xml&quot;" show="false"/>
    <elementParameter field="CHECK" name="USE_INLINE_CONTENT" value="true"/>
    <elementParameter field="MEMO" name="INLINE_CONTENT" value="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot;?>&#xA;&lt;databaseChangeLog &#xA;&#x9;xmlns=&quot;http://www.liquibase.org/xml/ns/dbchangelog&quot;&#xA;&#x9;xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&#xA;&#x9;xsi:schemaLocation=&quot;http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd&quot;>&#xA;&#xA;&#x9;&lt;changeSet author=&quot;talend&quot; id=&quot;Profile_Download_Layer&quot; failOnError=&quot;true&quot;  runOnChange = &quot;true&quot;>&#xA;&#x9;&#x9;&lt;sql>&#x9;  &#xA;&#x9;&#x9;&#x9;--DROP VIEW IF EXISTS argo_profile_map;&#xA;&#x9;&#x9;&#x9;--DROP VIEW IF EXISTS profile_download;&#xA;&#x9;&#x9;&#x9;--DROP VIEW IF EXISTS argo_profile_data CASCADE;&#xA;&#x9;&#x9;&#x9;DROP VIEW IF EXISTS profile_summary CASCADE;&#xA;&#xA;&#xA;&#x9;&#x9;&lt;/sql>&#xA;&#xA;&#x9;&#x9;&lt;sql>&#xA;&#x9;&#x9;&#x9;CREATE VIEW profile_summary AS (&#xA;&#x9;&#x9;&#x9;SELECT profile_general.*,&#xA;&#x9;&#x9;&#x9;CASE&#xA;&#x9;&#x9;&#x9;WHEN profile_general.data_centre::text ~ 'ME'::text THEN 'MEDS'::character varying&#xA;&#x9;&#x9;&#x9;WHEN profile_general.data_centre::text ~ 'IF'::text THEN 'CORIOLIS'::character varying&#xA;&#x9;&#x9;&#x9;WHEN profile_general.data_centre::text ~ 'CS'::text THEN 'CSIRO'::character varying&#xA;&#x9;&#x9;&#x9;WHEN profile_general.data_centre::text ~ 'IN'::text THEN 'INCOIS'::character varying&#xA;&#x9;&#x9;&#x9;WHEN profile_general.data_centre::text ~ 'AO'::text THEN 'AOML'::character varying&#xA;&#x9;&#x9;&#x9;WHEN profile_general.data_centre::text ~ 'NM'::text THEN 'NMDIS'::character varying&#xA;&#x9;&#x9;&#x9;WHEN profile_general.data_centre::text ~ 'HZ'::text THEN 'CSIO'::character varying&#xA;&#x9;&#x9;&#x9;WHEN profile_general.data_centre::text ~ 'JA'::text THEN 'JMA'::character varying&#xA;&#x9;&#x9;&#x9;WHEN profile_general.data_centre::text ~ 'KO'::text THEN 'KORDI'::character varying&#xA;&#x9;&#x9;&#x9;WHEN profile_general.data_centre::text ~ 'BO'::text THEN 'BODC'::character varying&#xA;&#x9;&#x9;&#x9;WHEN profile_general.data_centre::text ~ 'KM'::text THEN 'KMA'::character varying&#xA;&#x9;&#x9;&#x9;ELSE NULL::character varying&#xA;&#x9;&#x9;&#x9;END AS data_centre_name,   &#xA;&#xA;&#x9;&#x9;&#x9;CASE&#xA;&#x9;&#x9;&#x9;WHEN indexed_file.url::text ~ 'profiles/?.R'::text THEN 'Realtime'::character varying&#xA;&#x9;&#x9;&#x9;WHEN indexed_file.url::text ~ 'profiles/?.D'::text THEN 'Delayed'::character varying            &#xA;&#x9;&#x9;&#x9;END AS profile_processing_type,&#xA;&#x9;&#x9;&#x9;CASE&#xA;&#x9;&#x9;&#x9;WHEN indexed_file.url::text ~ 'profiles/M'::text THEN 'Merged-argo profile'::character varying&#xA;&#x9;&#x9;&#x9;WHEN indexed_file.url::text ~ 'profiles/B'::text THEN 'Bio-argo profile'::character varying            &#xA;&#x9;&#x9;&#x9;ELSE 'Core-argo profile'&#xA;&#x9;&#x9;&#x9;END AS profile_type,&#xA;&#xA;&#x9;&#x9;&#x9;exists(&#xA;&#x9;&#x9;&#x9;SELECT true&#xA;&#x9;&#x9;&#x9;from profile_params&#xA;&#x9;&#x9;&#x9;where profile_params.file_id = profile_general.file_id&#xA;&#x9;&#x9;&#x9;and profile_params.profile_no = profile_general.profile_no&#xA;&#x9;&#x9;&#x9;and station_parameters = 'DOXY') as oxygen_sensor&#xA;&#xA;&#x9;&#x9;&#x9;from profile_general, indexed_file WHERE  indexed_file.id = profile_general.file_id);&#xA;&#x9;&#x9;&lt;/sql>&#xA;&#xA;&#x9;&#x9;&lt;sql>&#xA;&#x9;&#x9;&#x9;CREATE VIEW argo_profile_data AS (&#xA;&#x9;&#x9;&#x9;SELECT&#xA;&#x9;&#x9;&#x9;url,size, &#xA;&#x9;&#x9;&#x9;data_centre_name, platform_number, &#xA;&#x9;&#x9;&#x9;cycle_number,  direction,&#xA;&#x9;&#x9;&#x9;data_state_indicator, &#xA;&#x9;&#x9;&#x9;data_mode, juld, juld_qc, &#xA;&#x9;&#x9;&#x9;juld_location, latitude, &#xA;&#x9;&#x9;&#x9;longitude, position_qc,&#xA;&#x9;&#x9;&#x9;position, oxygen_sensor, &#xA;&#x9;&#x9;&#x9;exists( SELECT true from  profile_summary where profile_type = 'Bio-argo profile'&#xA;&#x9;&#x9;&#x9;and profile_summary.file_id =  profile_summary.file_id  ) as bio_argo,&#xA;&#x9;&#x9;&#x9;project_name,&#xA;&#x9;&#x9;&#x9;level_no, pres, pres_qc, &#xA;&#x9;&#x9;&#x9;pres_adjusted, pres_adjusted_qc, &#xA;&#x9;&#x9;&#x9;pres_adjusted_error, temp, &#xA;&#x9;&#x9;&#x9;temp_qc, temp_adjusted, &#xA;&#x9;&#x9;&#x9;temp_adjusted_qc, temp_adjusted_error,&#xA;&#x9;&#x9;&#x9;psal, psal_qc, psal_adjusted, &#xA;&#x9;&#x9;&#x9;psal_adjusted_qc, psal_adjusted_error,&#xA;&#x9;&#x9;&#x9;doxy_adjusted, doxy_adjusted_qc, doxy_adjusted_error,&#xA;&#x9;&#x9;&#x9;temp_doxy, temp_doxy_adjusted, &#xA;&#x9;&#x9;&#x9;temp_doxy_qc, temp_doxy_adjusted_qc, &#xA;&#x9;&#x9;&#x9;temp_doxy_adjusted_error,&#xA;&#x9;&#x9;&#x9;bphase_doxy,bphase_doxy_qc,&#xA;&#x9;&#x9;&#x9;c1phase_doxy, c1phase_doxy_qc,&#xA;&#x9;&#x9;&#x9;c2phase_doxy, c2phase_doxy_qc,&#xA;&#x9;&#x9;&#x9;nitrate_adjusted, nitrate_adjusted_qc, nitrate_adjusted_error,&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;ph_in_situ_total_adjusted, ph_in_situ_total_adjusted_qc, ph_in_situ_total_adjusted_error,&#xA;&#x9;&#x9;&#x9;chla_adjusted, chla_adjusted_qc, chla_adjusted_error,&#xA;&#x9;&#x9;&#x9;fluorescence_chla_adjusted,  fluorescence_chla_adjusted_qc, fluorescence_chla_adjusted_error,&#xA;&#x9;&#x9;&#x9;bbp470, bbp470_qc, bbp470_adjusted, bbp470_adjusted_qc, bbp470_adjusted_error, &#xA;&#x9;&#x9;&#x9;bbp700, bbp700_qc, bbp700_adjusted, bbp700_adjusted_qc, bbp700_adjusted_error, &#xA;&#x9;&#x9;&#x9;beta_backscattering470, beta_backscattering470_qc, beta_backscattering470_adjusted, beta_backscattering470_adjusted_qc, beta_backscattering470_adjusted_error,&#xA;&#x9;&#x9;&#x9;beta_backscattering700, beta_backscattering700_qc, beta_backscattering700_adjusted, beta_backscattering700_adjusted_qc, beta_backscattering700_adjusted_error,&#xA;&#x9;&#x9;&#x9;down_irradiance380, down_irradiance380_qc, down_irradiance380_adjusted, down_irradiance380_adjusted_qc, down_irradiance380_adjusted_error, &#xA;&#x9;&#x9;&#x9;down_irradiance412, down_irradiance412_qc, down_irradiance412_adjusted, down_irradiance412_adjusted_qc, down_irradiance412_adjusted_error, &#xA;            down_irradiance490, down_irradiance490_qc, down_irradiance490_adjusted, down_irradiance490_adjusted_qc, down_irradiance490_adjusted_error,&#xA;            downwelling_par, downwelling_par_qc, downwelling_par_adjusted,  downwelling_par_adjusted_qc, downwelling_par_adjusted_error,&#xA;&#x9;&#x9;&#x9;profile_summary.profile_processing_type,&#xA;&#x9;&#x9;&#x9;profile_summary.profile_type,&#xA;&#x9;&#x9;&#x9;profile_summary.profile_no&#xA;&#x9;&#x9;&#x9;FROM &#xA;&#x9;&#x9;&#x9;profile_summary, &#xA;&#x9;&#x9;&#x9;profile_measurements,&#xA;&#x9;&#x9;&#x9;indexed_file&#xA;&#x9;&#x9;&#x9;WHERE profile_summary.file_id = profile_measurements.file_id&#xA;&#x9;&#x9;&#x9;and profile_summary.profile_no = profile_measurements.profile_no and indexed_file.id = profile_measurements.file_id&#xA;&#x9;&#x9;&#x9;) ;&#xA;&#x9;&#x9;&lt;/sql>   &#xA;&#xA;&#x9;&#x9;&lt;sql>&#xA;&#x9;&#x9;&#x9;CREATE VIEW argo_profile_map AS (&#xA;&#x9;&#x9;&#x9;SELECT &#xA;&#x9;&#x9;&#x9;profile_summary.*,&#xA;&#x9;&#x9;&#x9;indexed_file.*&#xA;&#xA;&#x9;&#x9;&#x9;from profile_summary&#xA;&#x9;&#x9;&#x9;left join indexed_file&#xA;&#x9;&#x9;&#x9;on file_id = indexed_file.id&#xA;&#x9;&#x9;&#x9;) ;&#xA;&#x9;&#x9;&lt;/sql>   &#xA;&#xA;&#x9;&#x9;&lt;sql>&#xA;&#x9;&#x9;&#x9;CREATE VIEW argo_primary_profile_core_low_res_good_qc_data AS (&#xA;&#x9;&#x9;&#x9;SELECT url,&#xA;&#x9;&#x9;&#x9;size,&#xA;&#x9;&#x9;&#x9;data_centre_name,&#xA;&#x9;&#x9;&#x9;oxygen_sensor,&#xA;&#x9;&#x9;&#x9;platform_number,&#xA;&#x9;&#x9;&#x9;cycle_number,&#xA;&#x9;&#x9;&#x9;direction,&#xA;&#x9;&#x9;&#x9;data_state_indicator,&#xA;&#x9;&#x9;&#x9;data_mode,&#xA;&#x9;&#x9;&#x9;juld,&#xA;&#x9;&#x9;&#x9;juld_qc,&#xA;&#x9;&#x9;&#x9;juld_location,&#xA;&#x9;&#x9;&#x9;latitude,&#xA;&#x9;&#x9;&#x9;longitude,&#xA;&#x9;&#x9;&#x9;position_qc,&#xA;&#x9;&#x9;&#x9;position,&#xA;&#x9;&#x9;&#x9;project_name,&#xA;&#x9;&#x9;&#x9;profile_processing_type,&#xA;&#x9;&#x9;&#x9;profile_