    <elementParameter field="MEMO" name="INLINE_CONTENT" value="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot;?>&#xA;&lt;databaseChangeLog &#xA;    xmlns=&quot;http://www.liquibase.org/xml/ns/dbchangelog&quot;&#xA;    xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&#xA;    xsi:schemaLocation=&quot;http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd&quot;>&#xA;&#xA;&lt;changeSet author=&quot;apberger&quot; id=&quot;create_plankton_products_metadata_summary_view&quot; runOnChange=&quot;true&quot;>&#xA;  &lt;sql>&#xA;    DROP VIEW IF EXISTS plankton_products_metadata_summary;&#xA;&#xA;    CREATE VIEW plankton_products_metadata_summary AS&#xA;    WITH p AS (&#xA;      SELECT '# ' || product || ',' || variable_name || ',' || cf_standard_name || ',' || long_name || ',' || imos_vocabulary_name || ',' || COALESCE(uv.name, '') || ',' || COALESCE(uv.short_name, '')&#xA;      FROM parameters_mapping pm&#xA;      LEFT JOIN units uv ON pm.unit_id = uv.id&#xA;      LEFT JOIN parameters p ON p.unique_id = pm.parameter_id&#xA;      WHERE facility = 'SOOP/ANMN' AND subfacility = 'CPR/NRS'&#xA;      ORDER BY variable_name , product)&#xA;      SELECT '# data_product_name' || ',' || 'data_column_name' || ',' || 'cf_standard_name' || ',' || 'long_name' || ',' || 'imos_vocabulary_name' || ',' || 'unit_name' || ',' || 'unit_short_name'&#xA;    UNION ALL&#xA;      SELECT * FROM p;&#xA;&#xA;&#x9;&lt;/sql>&#xA; &lt;/changeSet>&#xA;&#xA;&lt;changeSet author=&quot;bpasquer&quot; id=&quot;create_aodn_wave_dm_metadata_summary_view&quot; runOnChange=&quot;true&quot;>&#xA;  &lt;sql>&#xA;    DROP VIEW IF EXISTS aodn_wave_dm_metadata_summary;&#xA;&#xA;    CREATE VIEW aodn_wave_dm_metadata_summary AS&#xA;    WITH p AS (&#xA;      SELECT '# ' || variable_name || ',' || cf_standard_name || ',' || long_name || ',' || imos_vocabulary_name || ',' || COALESCE(uv.name, '') || ',' || COALESCE(uv.short_name, '') || ',' || COALESCE(uv.definition,'')&#xA;      FROM parameters_mapping pm&#xA;      LEFT JOIN units uv ON pm.unit_id = uv.id&#xA;      LEFT JOIN parameters p ON p.unique_id = pm.parameter_id&#xA;      WHERE facility = 'AODN' AND subfacility = 'WAVE' AND product = 'DM'&#xA;      ORDER BY variable_name , cf_standard_name , long_name , imos_vocabulary_name , uv.name , uv.short_name , uv.definition),&#xA;      q1 AS (&#xA;        SELECT DISTINCT '# ' || qc_scheme_short_name || ',' || flag_value || ',' || flag_meaning || ',' || flag_description&#xA;      FROM parameters_mapping pm&#xA;      LEFT JOIN qc_scheme qs1 ON qs1.qc_scheme_id = pm.qc_scheme_id&#xA;      LEFT JOIN qc_flags qf1 ON qf1.qc_scheme_id = qs1.qc_scheme_id&#xA;      WHERE facility = 'AODN' AND variable_name = 'wave_quality_control'&#xA;      ORDER BY '# ' || qc_scheme_short_name || ',' || flag_value || ',' || flag_meaning || ',' || flag_description),   &#xA;      q2 AS (&#xA;        SELECT DISTINCT '# ' || qc_scheme_short_name || ',' || flag_value || ',' || flag_meaning || ',' || flag_description&#xA;      FROM parameters_mapping pm&#xA;      LEFT JOIN qc_scheme qs2 ON qs2.qc_scheme_id = pm.qc_scheme_id&#xA;      LEFT JOIN qc_flags qf2 ON qf2.qc_scheme_id = qs2.qc_scheme_id&#xA;      WHERE facility = 'AODN' AND variable_name = 'wave_subflag'&#xA;      ORDER BY '# ' || qc_scheme_short_name || ',' || flag_value || ',' || flag_meaning || ',' || flag_description),&#xA;      q3 AS (&#xA;        SELECT DISTINCT '# ' || qc_scheme_short_name || ',' || flag_value || ',' || flag_meaning || ',' || flag_description&#xA;      FROM parameters_mapping pm&#xA;      LEFT JOIN qc_scheme qs3 ON qs3.qc_scheme_id = pm.qc_scheme_id&#xA;      LEFT JOIN qc_flags qf3 ON qf3.qc_scheme_id = qs3.qc_scheme_id&#xA;      WHERE facility = 'AODN' AND variable_name = 'TEMP_subflag'&#xA;      ORDER BY '# ' || qc_scheme_short_name || ',' || flag_value || ',' || flag_meaning || ',' || flag_description)&#xA;      SELECT '# data_column_name' || ',' || 'cf_standard_name' || ',' || 'long_name' || ',' || 'imos_vocabulary_name' || ',' || 'unit_name' || ',' || 'unit_short_name'&#xA;    UNION ALL&#xA;      SELECT * FROM p&#xA;    UNION ALL&#xA;      SELECT '#'&#xA;    UNION ALL&#xA;      SELECT '# Primary flags: wave_quality_control and TEMP_quality_control'&#xA;    UNION ALL&#xA;      SELECT '# qc_scheme_short_name' || ',' || 'flag_value' || ',' || 'flag_meaning' || ',' || 'flag_description'&#xA;    UNION ALL&#xA;      SELECT * FROM q1&#xA;    UNION ALL&#xA;      SELECT '#'&#xA;    UNION ALL&#xA;      SELECT '# Secondary flag: wave_subflag' &#xA;    UNION ALL&#xA;      SELECT '# qc_scheme_short_name' || ',' || 'flag_value' || ',' || 'flag_meaning' || ',' || 'flag_description'   &#xA;    UNION ALL&#xA;      SELECT * FROM q2&#xA;    UNION ALL&#xA;      SELECT '#'&#xA;    UNION ALL&#xA;      SELECT '# Secondary flag: TEMP_subflag' &#xA;    UNION ALL&#xA;      SELECT '# qc_scheme_short_name' || ',' || 'flag_value' || ',' || 'flag_meaning' || ',' || 'flag_description'&#xA;    UNION ALL&#xA;      SELECT * FROM q3;  &#xA; &#xA;  &lt;/sql>&#xA; &lt;/changeSet>&#xA;&#xA;&lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_aatams_biologging_penguin_view&quot; runOnChange=&quot;true&quot;>&#xA;  &lt;sql>&#xA;    DROP VIEW IF EXISTS aatams_biologging_penguin_metadata_summary;&#xA;&#xA;    CREATE VIEW aatams_biologging_penguin_metadata_summary AS&#xA;    WITH p AS (&#xA;      SELECT '# ' || variable_name || ',' || cf_standard_name || ',' || imos_vocabulary_name || ',' || uv.name || ',' || uv.short_name || ',' || uv.definition&#xA;      FROM parameters_mapping pm&#xA;      LEFT JOIN units uv ON pm.unit_id = uv.id&#xA;      LEFT JOIN parameters p ON p.unique_id = pm.parameter_id&#xA;      WHERE facility = 'AATAMS' AND subfacility = 'biologging' AND product = 'penguin'&#xA;      ORDER BY variable_name || ',' || cf_standard_name || ',' || imos_vocabulary_name || ',' || uv.name || ',' || uv.short_name || ',' || uv.definition)&#xA;      SELECT '# data_column_name' || ',' || 'cf_standard_name' || ',' || 'imos_vocabulary_name' || ',' || 'unit_name' || ',' || 'unit_short_name' || ',' || 'uv.definition'&#xA;    UNION ALL&#xA;      SELECT * FROM p WHERE &quot;?column?&quot; IS NOT NULL;&#xA;&#xA;&#x9;&lt;/sql>&#xA; &lt;/changeSet>&#xA;&#xA;&lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_aatams_biologging_shearwater_metadata_summary_view&quot; runOnChange=&quot;true&quot;>&#xA;  &lt;sql>&#xA;    DROP VIEW IF EXISTS aatams_biologging_shearwater_metadata_summary;&#xA;&#xA;    CREATE VIEW aatams_biologging_shearwater_metadata_summary AS&#xA;    WITH p AS (&#xA;      SELECT '# ' || variable_name || ',' || cf_standard_name || ',' || imos_vocabulary_name || ',' || uv.name || ',' || uv.short_name || ',' || uv.definition&#xA;      FROM parameters_mapping pm&#xA;      LEFT JOIN units uv ON pm.unit_id = uv.id&#xA;      LEFT JOIN parameters p ON p.unique_id = pm.parameter_id&#xA;      WHERE facility = 'AATAMS' AND subfacility = 'biologging' AND product = 'shearwater'&#xA;      ORDER BY variable_name || ',' || cf_standard_name || ',' || imos_vocabulary_name || ',' || uv.name || ',' || uv.short_name || ',' || uv.definition),&#xA;     a AS (SELECT DISTINCT animal_id,substring(animal_id,'(.*)-')::text AS id, release_date FROM aatams_biologging_shearwater.aatams_biologging_shearwater_map ORDER BY substring(animal_id,'(.*)-')),&#xA;     b AS (SELECT '# ' || a.animal_id || ',' || tag_type || ',' || tag_code || ',' || tag_id || ',' || round(release_longitude::numeric,3) || ',' || round(release_latitude::numeric,3) || ',' || &#xA;    COALESCE(mass_at_release::text,'') || ',' || COALESCE(round(culman_length::numeric,1)::text,'') || ',' || COALESCE(round(culman_height::numeric,1)::text,'')&#xA;    FROM a&#xA;    JOIN aatams_biologging_shearwater.aatams_biologging_shearwater_metadata m ON m.tag_id::text = a.id AND m.release_date = a.release_date&#xA;    ORDER BY a.animal_id)&#xA;      SELECT '# data_column_name' || ',' || 'cf_standard_name' || ',' || 'imos_vocabulary_name' || ',' || 'unit_name' || ',' || 'unit_short_name' || ',' || 'uv.definition'&#xA;    UNION ALL&#xA;      SELECT * FROM p WHERE &quot;?column?&quot; IS NOT NULL&#xA;    UNION ALL&#xA;      SELECT '#'&#xA;    UNION ALL&#xA;      SELECT '# animal_id' || ',' || 'tag_type' || ',' || 'tag_code' || ',' || 'tag_id' || ',' || 'release_longitude' || ',' || 'release_latitude' || ',' || 'mass_at_release_g' || ',' ||&#xA;      'culman_length_cm' || ',' || 'culman_height_cm'&#xA;    UNION ALL&#xA;      SELECT * FROM b;&#xA;&#xA;&#x9;&lt;/sql>&#xA; &lt;/changeSet>&#xA;&#xA;&lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_aatams_biologging_snowpetrel_metadata_summary_view&quot; runOnChange=&quot;true&quot;>&#xA;  &lt;sql>&#xA;    DROP VIEW IF EXISTS aatams_biologging_snowpetrel_metadata_summary;&#xA;&#xA;    CREATE VIEW aatams_biologging_snowpetrel_metadata_summary AS&#xA;    WITH p AS (&#xA;      SELECT '# ' || variable_name || ',' || cf_standard_name || ',' || imos_vocabulary_name || ',' || uv.name || ',' || uv.short_name || ',' || uv.definition&#xA;      FROM parameters_mapping pm&#xA;      LEFT JOIN units uv ON pm.unit_id = uv.id&#xA;      LEFT JOIN parameters p ON p.unique_id = pm.parameter_id&#xA;      WHERE facility = 'AATAMS' AND subfacility = 'biologging' AND product = 'shearwater'&#xA;      ORDER BY variable_name || ',' || cf_standard_name || ',' || imos_vocabulary_name || ',' || uv.name || ',' || uv.short_name || ',' || uv.definition),&#xA;     a AS (SELECT DISTINCT animal_id FROM aatams_biologging_snowpetrel.aatams_biologging_snowpetrel_map ORDER BY animal_id),&#xA;     b AS (SELECT '# ' || a.animal_id || ',' || tag_type || ',' || tag_code || ',' || tag_id || ',' || round(release_longitude::numeric,3) || ',' || round(release_latitude::numeric,3) || ',' || &#xA;    COALESCE(mass_at_release::text,'') || ',' || COALESCE(round(culman_length::numeric,1)::text,'') || ',' || COALESCE(round(culman_height::numeric,1)::text,'')&#xA;    FROM a&#xA;    JOIN aatams_biologging_snowpetrel.aatams_biologging_snowpetrel_metadata m ON m.tag_id::text = a.animal_id&#xA;    ORDER BY a.animal_id)&#xA;      SELECT '# data_column_name' || ',' || 'cf_standard_name' || ',' || 'imos_vocabulary_name' || ',' || 'unit_name' || ',' || 'unit_short_name' || ',' || 'uv.definition'&#xA;    UNION ALL&#xA;      SELECT * FROM p WHERE &quot;?column?&quot; IS NOT NULL&#xA;    UNION ALL&#xA;      SELECT '#'&#xA;    UNION ALL&#xA;      SELECT '# animal_id' || ',' || 'tag_type' || ',' || 'tag_code' || ',' || 'tag_id' || ',' || 'release_longitude' || ',' || 'release_latitude' || ',' || 'mass_at_release_g' || ',' ||&#xA;      'culman_length_cm' || ',' || 'culman_height_cm'&#xA;    UNION ALL&#xA;      SELECT * FROM b;&#xA;&#xA;&#x9;&lt;/sql>&#xA; &lt;/changeSet>&#xA;&#xA;&#xA;&lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_aatams_sattag_dm_metadata_summary_view&quot; runOnChange=&quot;true&quot;>&#xA;  &lt;sql>&#xA;    DROP VIEW IF EXISTS aatams_sattag_dm_metadata_summary;&#xA;&#xA;    CREATE VIEW aatams_sattag_dm_metadata_summary AS&#xA;    WITH p AS (&#xA;      SELECT '# ' || variable_name || ',' || cf_standard_name || ',' || imos_vocabulary_name || ',' || uv.name || ',' || uv.short_name || ',' || uv.definition&#xA;      FROM parameters_mapping pm&#xA;      LEFT JOIN units uv ON pm.unit_id = uv.id&#xA;      LEFT JOIN parameters p ON p.unique_id = pm.parameter_id&#xA;      WHERE facility = 'AATAMS' AND subfacility = 'sattag' AND product = 'dm'&#xA;      ORDER BY variable_name || ',' || cf_standard_name || ',' || imos_vocabulary_name || ',' || uv.name || ',' || uv.short_name || ',' || uv.definition),&#xA;     a AS (SELECT DISTINCT device_id FROM aatams_sattag_dm.aatams_sattag_dm_profile_map ORDER BY device_id),&#xA;     b AS (SELECT '# ' || a.device_id || ',' || m.device_wmo_ref || ',' || m.tag_type || ',' || m.common_name || ',' || m.release_site || ',' || COALESCE(m.release_lon::text,'') || ',' || COALESCE(m.release_lat::text,'') || ',' || m.state_country  || ',' || &#xA;      COALESCE(date(m.release_date)::text,'') || ',' || COALESCE(date(m.recovery_date)::text,'') || ',' || m.age_class || ',' || m.sex || ',' || COALESCE(m.length::text,'') || ',' || &#xA;      COALESCE(m.girth::text,'') || ',' || COALESCE(m.estimated_mass::text,'') || ',' || COALESCE(m.actual_mass::text,'') || ',' || m.sample || ',' || m.comment || ',' || m.pi || ',' || m.institution&#xA;      FROM a&#xA;      LEFT JOIN aatams_sattag_nrt.aatams_sattag_nrt_metadata m ON a.device_id = m.device_id&#xA;      where ('# ' || a.device_id || ',' || m.device_wmo_ref || ',' || m.tag_type || ',' || m.common_name || ',' || m.release_site || ',' || COALESCE(m.release_lon::text,'') || ',' || COALESCE(m.release_lat::text,'') || ',' || m.state_country  || ',' || &#xA;      COALESCE(date(m.release_date)::text,'') || ',' || COALESCE(date(m.recovery_date)::text,'') || ',' || m.age_class || ',' || m.sex || ',' || COALESCE(m.length::text,'') || ',' || &#xA;      COALESCE(m.girth::text,'') || ',' || COALESCE(m.estimated_mass::text,'') || ',' || COALESCE(m.actual_mass::text,'') || ',' || m.sample || ',' || m.comment || ',' || m.pi || ',' || m.institution) != ''&#xA;        ORDER BY a.device_id)&#xA;      SELECT '# data_column_name' || ',' || 'cf_standard_name' || ',' || 'imos_vocabulary_name' || ',' || 'unit_name' || ',' || 'unit_short_name' || ',' || 'uv.definition'&#xA;    UNION ALL&#xA;      SELECT * FROM p WHERE &quot;?column?&quot; IS NOT NULL&#xA;    UNION ALL&#xA;      SELECT '#'&#xA;    UNION ALL&#xA;      SELECT '# device_id' || ',' || 'device_wmo_ref' || ',' || 'tag_type' || ',' || 'common_name' || ',' || 'release_site' || ',' || 'release_lon' || ',' ||&#xA;      'release_lat' || ',' || 'state_country' || ',' || 'release_date' || ',' || 'recovery_date' || ',' || 'age_class' || ',' || 'sex' || ',' ||&#xA;      'length' || ',' || 'girth' || ',' || 'estimated_mass' || ',' || 'actual_mass' || ',' || 'sample' || ',' || 'comment' || ',' || 'principal investigator' || ',' || 'institution'&#xA;    UNION ALL&#xA;      SELECT * FROM b;&#xA;&#xA;&#x9;&lt;/sql>&#xA; &lt;/changeSet>&#xA;&#xA;&#xA;&lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_aatams_sattag_nrt_metadata_summary_view&quot; runOnChange=&quot;true&quot;>&#xA;  &lt;sql>&#xA;    DROP VIEW IF EXISTS aatams_sattag_nrt_metadata_summary;&#xA;&#xA;    CREATE VIEW aatams_sattag_nrt_metadata_summary AS&#xA;    WITH p AS (&#xA;      SELECT '# ' || variable_name || ',' || cf_standard_name || ',' || imos_vocabulary_name || ',' || uv.name || ',' || uv.short_name || ',' || uv.definition&#xA;      FROM parameters_mapping pm&#xA;      LEFT JOIN units uv ON pm.unit_id = uv.id&#xA;      LEFT JOIN parameters p ON p.unique_id = pm.parameter_id&#xA;      WHERE facility = 'AATAMS' AND subfacility = 'sattag' AND product = 'nrt'&#xA;      ORDER BY variable_name || ',' || cf_standard_name || ',' || imos_vocabulary_name || ',' || uv.name || ',' || uv.short_name || ',' || uv.definition),&#xA;     a AS (SELECT DISTINCT device_id FROM aatams_sattag_nrt.aatams_sattag_nrt_profile_map ORDER BY device_id),&#xA;     b AS (SELECT '# ' || a.device_id || ',' || m.device_wmo_ref || ',' || m.tag_type || ',' || m.common_name || ',' || m.release_site || ',' || COALESCE(m.release_lon::text,'') || ',' || COALESCE(m.release_lat::text,'') || ',' || m.state_country  || ',' || &#xA;      COALESCE(date(m.release_date)::text,'') || ',' || COALESCE(date(m.recovery_date)::text,'') || ',' || m.age_class || ',' || m.sex || ',' || COALESCE(m.length::text,'') || ',' || &#xA;      COALESCE(m.girth::text,'') || ',' || COALESCE(m.estimated_mass::text,'') || ',' || COALESCE(m.actual_mass::text,'') || ',' || m.sample || ',' || m.comment || ',' || m.pi || ',' || m.institution&#xA;      FROM a&#xA;      LEFT JOIN aatams_sattag_nrt.aatams_sattag_nrt_metadata m ON a.device_id = m.device_id&#xA;        ORDER BY a.device_id)&#xA;      SELECT '# data_column_name' || ','