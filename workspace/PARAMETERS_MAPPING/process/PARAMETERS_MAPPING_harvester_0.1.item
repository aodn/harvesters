    <elementParameter field="MEMO" name="INLINE_CONTENT" value="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot;?>&#xA;&lt;databaseChangeLog &#xA;    xmlns=&quot;http://www.liquibase.org/xml/ns/dbchangelog&quot;&#xA;    xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&#xA;    xsi:schemaLocation=&quot;http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd&quot;>&#xA;&#xA;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_parameters_table&quot;>&#xA;        &lt;sql>&#xA;CREATE TABLE parameters&#xA;(&#xA;  unique_id integer NOT NULL,&#xA;  cf_standard_name text,&#xA;  cf_alias_name text,&#xA;  long_name text,&#xA;  imos_vocabulary_name text,&#xA;  imos_vocabulary_id integer,&#xA;  CONSTRAINT parameters_pkey PRIMARY KEY (unique_id)&#xA;);&#xA;        &lt;/sql>&#xA;    &lt;/changeSet>&#xA;   &#xA;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_units_table&quot;>&#xA;        &lt;sql>&#xA;CREATE TABLE units&#xA;(&#xA;  id integer NOT NULL,&#xA;  name text,&#xA;  short_name text,&#xA;  definition text,&#xA;  date_added date,&#xA;  CONSTRAINT units_pkey PRIMARY KEY (id)&#xA;);&#xA;        &lt;/sql>&#xA;    &lt;/changeSet>&#xA;&#xA;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_qc_scheme_table&quot;>&#xA;        &lt;sql>&#xA;CREATE TABLE qc_scheme&#xA;(&#xA;  qc_scheme_id integer NOT NULL,&#xA;  qc_scheme_short_name text,&#xA;  qc_scheme_definition text,&#xA;  CONSTRAINT qc_scheme_pkey PRIMARY KEY (qc_scheme_id)&#xA;);&#xA;        &lt;/sql>&#xA;    &lt;/changeSet>&#xA;       &#xA;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_parameters_mapping_table&quot;>&#xA;        &lt;sql>&#xA;CREATE TABLE parameters_mapping&#xA;(&#xA;  facility text NOT NULL,&#xA;  subfacility text NOT NULL,&#xA;  product text NOT NULL,&#xA;  variable_name text NOT NULL,&#xA;  parameter_id integer,&#xA;  unit_id integer,&#xA;  unit_info text,&#xA;  qc_scheme_id integer,&#xA;  CONSTRAINT parameters_mapping_pkey PRIMARY KEY (facility, subfacility, product, variable_name),&#xA;  CONSTRAINT parameters_mapping_fkey_units FOREIGN KEY (unit_id)&#xA;      REFERENCES units (id) MATCH SIMPLE&#xA;      ON UPDATE CASCADE ON DELETE CASCADE,&#xA;  CONSTRAINT parameters_mapping_fkey_parameters FOREIGN KEY (parameter_id)&#xA;      REFERENCES parameters (unique_id) MATCH SIMPLE&#xA;      ON UPDATE CASCADE ON DELETE CASCADE,&#xA;  CONSTRAINT parameters_mapping_fkey_qc_scheme FOREIGN KEY (qc_scheme_id)&#xA;      REFERENCES qc_scheme (qc_scheme_id) MATCH SIMPLE&#xA;      ON UPDATE CASCADE ON DELETE CASCADE&#xA;);&#xA;        &lt;/sql>&#xA;    &lt;/changeSet>&#xA;     &#xA;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_qc_flags_table&quot;>&#xA;        &lt;sql>&#xA;CREATE TABLE qc_flags&#xA;(&#xA;  id integer NOT NULL,&#xA;  qc_scheme_id integer,&#xA;  flag_value text,&#xA;  flag_meaning text,&#xA;  flag_description text,&#xA;  CONSTRAINT qc_flags_pkey PRIMARY KEY (id),&#xA;  CONSTRAINT qc_flags_fkey FOREIGN KEY (qc_scheme_id)&#xA;      REFERENCES qc_scheme (qc_scheme_id) MATCH SIMPLE&#xA;      ON UPDATE CASCADE ON DELETE CASCADE&#xA;);&#xA;        &lt;/sql>&#xA;    &lt;/changeSet>&#xA;   &lt;changeSet author=&quot;bpasquer&quot; id=&quot;modify_parameters_table&quot;>&#xA; &lt;sql>&#xA;   ALTER TABLE parameters&#xA;   ADD comment text;&#xA;        &lt;/sql>&#xA;    &lt;/changeSet>&#xA;&lt;/databaseChangeLog> "/>
