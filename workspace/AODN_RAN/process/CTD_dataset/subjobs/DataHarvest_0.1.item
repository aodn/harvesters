    <elementParameter field="FILE" name="FILENAME" value="&quot;/media/Laurent_emII/TOS_DI-r96646-V5.1.3/workspace_sdi/changelog.xml&quot;"/>
    <elementParameter field="CHECK" name="USE_INLINE_CONTENT" value="true"/>
    <elementParameter field="MEMO" name="INLINE_CONTENT" value="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot; standalone=&quot;no&quot;?>&#xA;&lt;databaseChangeLog xmlns=&quot;http://www.liquibase.org/xml/ns/dbchangelog&quot; xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot; xsi:schemaLocation=&quot;http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd&quot;>&#xA;&#xA; &lt;changeSet author=&quot;talend&quot; id=&quot;cr_deployments-5&quot;>&#xA;       &lt;sql>&#xA;       CREATE TABLE deployments&#xA;(&#xA;  file_id integer NOT NULL,&#xA;  time_coverage_start timestamp with time zone,&#xA;  time_coverage_end  timestamp with time zone,&#xA;  geospatial_vertical_min  character varying(20),&#xA;  geospatial_vertical_max character varying(20),&#xA;  cruise_id character varying(20),&#xA;  CONSTRAINT deployments_pkey PRIMARY KEY (file_id ),&#xA;  CONSTRAINT fk_file_id FOREIGN KEY (file_id)&#xA;      REFERENCES indexed_file (id) MATCH SIMPLE&#xA;      ON UPDATE CASCADE ON DELETE CASCADE)&#xA;       &lt;/sql>&#xA;    &lt;/changeSet>&#xA;&#xA;&#xA;&lt;changeSet author=&quot;talend&quot; id=&quot;measurements-1&quot;>&#xA;       &lt;sql>&#xA;    CREATE TABLE measurements&#xA;(&#xA;  measurement_id bigserial,&#xA;  file_id integer NOT NULL,&#xA;  time timestamp with time zone,&#xA;  time_qc_flag character varying(1),&#xA;  latitude double precision,&#xA;  longitude double precision,&#xA;  position_qc_flag character varying(1),&#xA;  pressure double precision,&#xA;  profile integer,&#xA;  salinity double precision,&#xA;  salinity_qc_flag character varying(1),&#xA;  salinity_sd_test character varying(1),&#xA;  salinity_whole_profile_flag character varying(1),&#xA;  temperature double precision,&#xA;  temperature_qc_flag character varying(1),&#xA;  temperature_sd_test character varying(1),&#xA;  temperature_whole_profile_flag character varying(1), &#xA;  geom geometry(GEOMETRY,4326) CHECK (ST_IsValid(geom)),&#xA;  CONSTRAINT measurements_pkey PRIMARY KEY (measurement_id),&#xA;  CONSTRAINT fk_file_id FOREIGN KEY (file_id)&#xA;      REFERENCES indexed_file (id) MATCH SIMPLE&#xA;      ON UPDATE CASCADE ON DELETE CASCADE&#xA;)&#xA;       &lt;/sql>&#xA;    &lt;/changeSet>&#xA;&#xA;&lt;changeSet author=&quot;talend&quot; id=&quot;index_measurments-6&quot;>&#xA;       &lt;sql>&#xA;        DROP INDEX IF EXISTS measurements_file_id_idx;&#xA;        DROP INDEX IF EXISTS measurements_T_idx;&#xA;&#x9;CREATE INDEX measurements_file_id_idx ON measurements (file_id ASC NULLS LAST);&#xA;&#x9;CREATE INDEX measurements_T_idx ON measurements (file_id,timezone('UTC'::text, measurements.time));&#xA;       &lt;/sql>&#xA;    &lt;/changeSet>&#xA;&#xA;&#xA;&lt;changeSet author=&quot;talend&quot; id=&quot;aodn_ran_ctd_data_view-1&quot; runOnChange=&quot;true&quot;>&#xA;        &lt;sql>&#xA;&#x9;DROP VIEW IF EXISTS aodn_ran_ctd_data;&#xA;&#x9;CREATE VIEW aodn_ran_ctd_data AS &#xA;&#x9;SELECT measurements.file_id, &#xA;        d.cruise_id,&#xA;&#x9;measurements.geom,&#xA;&#x9;measurements.time AT TIME ZONE 'UTC' AS time,&#xA;   &#x9;measurements.latitude, &#xA;&#x9;measurements.longitude,&#xA;&#x9;measurements. pressure,&#xA;&#x9;measurements. profile,&#xA;&#x9;measurements. salinity,&#xA;&#x9;measurements. temperature,&#xA;&#x9;measurements. temperature_qc_flag,&#xA;&#x9;measurements. temperature_sd_test,&#xA;&#x9;measurements. temperature_whole_profile_flag,&#xA;&#x9;measurements. salinity_qc_flag,&#xA;&#x9;measurements. salinity_whole_profile_flag,&#xA;&#x9;measurements.salinity_sd_test,&#xA;&#x9;measurements.position_qc_flag,&#xA;&#x9;measurements.time_qc_flag&#xA;&#x9;FROM measurements &#xA;JOIN deployments d ON  measurements.file_id =d.file_id&#xA;ORDER BY measurements.file_id,timezone('UTC'::text, measurements.time);&#xA;&#x9;&lt;/sql>&#xA; &lt;/changeSet>&#xA;&#xA;&#xA;&lt;changeSet author=&quot;talend&quot; id=&quot;add_cruise_colour-2&quot; >&#xA;        &lt;sql>&#xA;&#x9;ALTER TABLE deployments&#xA;&#x9;ADD colour character varying(7)&#xA;&#x9;&lt;/sql>&#xA; &lt;/changeSet> &#xA; &#xA;&lt;changeSet author=&quot;talend&quot; id=&quot;aodn_ran_ctd_map_view-1&quot; runOnChange=&quot;true&quot;>&#xA;        &lt;sql>&#xA; DROP VIEW IF EXISTS aodn_ran_ctd_map;&#xA;CREATE VIEW aodn_ran_ctd_map AS(&#xA;     WITH a AS(SELECT DISTINCT file_id,latitude,longitude,geom &#xA;FROM measurements &#xA;where position_qc_flag NOT IN('3','4')&#xA;GROUP BY file_id,latitude,longitude,geom),&#xA;b AS( SELECT d.file_id,&#xA;    replace(f.url::text, 'dodsC'::text, 'fileServer'::text) AS url,&#xA;    f.size,&#xA;    timezone('UTC'::text, d.time_coverage_start) AS time_coverage_start,&#xA;    timezone('UTC'::text, d.time_coverage_end) AS time_coverage_end,&#xA;    d.cruise_id,&#xA;    d.geospatial_vertical_min,&#xA;    d.geospatial_vertical_max,&#xA;    d.colour&#xA;   FROM deployments d&#xA;   JOIN indexed_file f ON d.file_id = f.id)&#xA;   &#xA; SELECT b.file_id,&#xA;    b.url,&#xA;    b.size,&#xA;    b.time_coverage_start,&#xA;    b.time_coverage_end,&#xA;    b.cruise_id,&#xA;    b.geospatial_vertical_min,&#xA;    b.geospatial_vertical_max,&#xA;    b.colour,&#xA;    a.latitude,&#xA;    a.longitude,&#xA;    a.geom&#xA;    from b&#xA;    JOIN a ON a.file_id=b.file_id&#xA;    ORDER BY cruise_id,timezone('UTC'::text, b.time_coverage_start) )&#xA;&#x9;&lt;/sql>&#xA; &lt;/changeSet>&#xA;&#xA;&#xA;&lt;changeSet author=&quot;talend&quot; id=&quot;gist_idx&quot;>&#xA;        &lt;sql>&#xA;CREATE INDEX measurement_gist_idx ON measurements USING gist(geom);&#xA;&#x9;&lt;/sql>&#xA; &lt;/changeSet>&#xA; &#xA;&#xA;&lt;changeSet author=&quot;talend&quot; id=&quot;deployment_idx&quot;>&#xA;        &lt;sql>&#xA;CREATE INDEX deployment_idx ON deployments USING btree(cruise_id,timezone('UTC'::text, time_coverage_start));&#xA; &#xA;&#x9;&lt;/sql>&#xA; &lt;/changeSet>&#xA;&lt;/databaseChangeLog>&#xA;&#xA;"/>
